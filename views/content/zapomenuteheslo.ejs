<form method="POST" id="RegLog">
    <input type="text" name="username" placeholder="Zadejte vaši přezdívku" required>
    <input type="email" name="email" placeholder="Zadejte vaši emailovou adresu" required>
        <% errory.forEach(function(error){ %>
            <div class="error"><%= error %></div>
        <% }); %>
    <input type="submit" value="Požádat o resetování">
</form>
<h3>Již víš heslo? <a href="./login">Přihlaš se</a></h3>